{
  "name": "projects-duse-example-pc-ui-sample",
  "$schema": "../../../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/projects/duse/example/pc/ui/sample/src",
  "projectType": "library",
  "tags": ["type:ui"],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/projects/duse/example/pc/ui/sample/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/projects/duse/example/pc/ui/sample"],
      "options": {
        "jestConfig": "libs/projects/duse/example/pc/ui/sample/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "port": 4400,
        "config": {
          "configFolder": "libs/projects/duse/example/pc/ui/sample/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/react",
        "outputPath": "dist/storybook/projects-duse-example-pc-ui-sample",
        "config": {
          "configFolder": "libs/projects/duse/example/pc/ui/sample/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
