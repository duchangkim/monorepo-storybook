      "@duse/projects/duse/example/pc/pages/login": ["libs/projects/duse/example/pc/pages/login/src/index.ts"],
      "@duse/projects/duse/example/pc/service": ["libs/projects/duse/example/pc/service/src/index.ts"],
      "@duse/projects/duse/example/pc/ui/login-form": ["libs/projects/duse/example/pc/ui/login-form/src/index.ts"],
      "@duse/projects/duse/example/pc/ui/sample": ["libs/projects/duse/example/pc/ui/sample/src/index.ts"],
      "@duse/projects/duse/example/pc/util": ["libs/projects/duse/example/pc/util/src/index.ts"],
      "@duse/react": ["libs/plugin/react/src/index.ts"],
      "@duse/shared/constant/*": ["libs/shared/constant/src/*"],
