      "@duse/shared/service/api-service": ["libs/shared/service/api-service/src/index.ts"],
      "@duse/shared/service/local-storage-service": ["libs/shared/service/local-storage-service/src/index.ts"],
      "@duse/shared/type": ["libs/shared/type/src/index.ts"],
      "@duse/shared/util": ["libs/shared/util/src/index.ts"],
      "@duse/workspace": ["libs/plugin/workspace/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp", "dist"]
}
